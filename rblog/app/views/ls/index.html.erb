<p id="notice"><%= notice %></p>

<h1>Listing Ls</h1>

<table>
  <thead>
    <tr>
      <th>A</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ls.each do |l| %>
      <tr>
        <td><%= l.a %></td>
        <td><%= link_to 'Show', l %></td>
        <td><%= link_to 'Edit', edit_l_path(l) %></td>
        <td><%= link_to 'Destroy', l, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New L', new_l_path %>
